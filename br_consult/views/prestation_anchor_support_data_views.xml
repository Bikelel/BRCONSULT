            <field name="name">prestation.anchor.support.data.list</field>
            <field name="model">prestation.anchor.support.data</field>
            <field name="arch" type="xml">
                <tree string="Nature des supports d’ancrage" multi_edit="1" editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_prestation_anchor_support_data" model="ir.actions.act_window">
            <field name="name">Nature des supports d’ancrage</field>
            <field name="res_model">prestation.anchor.support.data</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Nature des supports d’ancrage" 
              id="br_consult_menu_prestation_anchor_support_data" 
              parent="br_consult_settings_menu"
              sequence="50"
              action="action_prestation_anchor_support_data"/>

  </data>
</odoo>