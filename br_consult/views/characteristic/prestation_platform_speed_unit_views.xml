<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_prestation_platform_speed_unit_list" model="ir.ui.view">
            <field name="name">prestation.platform.speed.unit.list</field>
            <field name="model">prestation.platform.speed.unit</field>
            <field name="arch" type="xml">
                <tree string="Unité de vitesse verticale" multi_edit="1" editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_prestation_platform_speed_unit" model="ir.actions.act_window">
            <field name="name">Unité de vitesse verticale</field>
            <field name="res_model">prestation.platform.speed.unit</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Unité de vitesse verticale" 
              id="br_consult_menu_prestation_platform_speed_unit" 
              parent="br_consult_settings_levage_data_menu"
              sequence="50"
              action="action_prestation_platform_speed_unit"/>

  </data>
</odoo>