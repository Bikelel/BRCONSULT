            <field name="name">prestation.platform.fixation.mat.list</field>
            <field name="model">prestation.platform.fixation.mat</field>
            <field name="arch" type="xml">
                <tree string="fixation de la plateforme MAT" multi_edit="1" editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_prestation_platform_fixation_mat" model="ir.actions.act_window">
            <field name="name">fixation de la plateforme mat</field>
            <field name="res_model">prestation.platform.fixation.mat</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="fixation de la plateforme MAT" 
              id="br_consult_menu_prestation_platform_fixation_mat" 
              parent="br_consult_settings_levage_data_menu"
              sequence="50"
              action="action_prestation_platform_fixation_mat"/>

  </data>
</odoo>