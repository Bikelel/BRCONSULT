<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_prestation_approach_floor_width_list" model="ir.ui.view">
            <field name="name">prestation.approach.floor.width.list</field>
            <field name="model">prestation.approach.floor.width</field>
            <field name="arch" type="xml">
                <tree string="Motif" multi_edit="1" editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_prestation_approach_floor_width" model="ir.actions.act_window">
            <field name="name">Largeur plancher d'approche</field>
            <field name="res_model">prestation.approach.floor.width</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Largeur plancher d'approche" 
              id="br_consult_menu_prestation_approach_floor_width" 
              parent="br_consult_settings_menu"
              sequence="50"
              action="action_prestation_approach_floor_width"/>

  </data>
</odoo>